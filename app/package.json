{
  "name": "astro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "watch": "node node_modules/typescript/bin/tsc -w",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "android-staging": "node node_modules/react-native/local-cli/cli.js run-android --variant=releaseStaging",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "release-ios": "react-native unbundle --entry-file index.ios.js --platform ios --dev false",
    "release-android": "node node_modules/typescript/bin/tsc && cd android && gradlew assembleRelease",
    "push-android": "code-push release-react astro-android android --deploymentName Staging",
    "push-ios": "code-push release-react astro-ios ios --deploymentName Staging",
    "push-android-prod": "code-push release-react astro-android android --deploymentName Production",
    "push-ios-prod": "code-push release-react astro-ios ios --deploymentName Production",
    "push-key": "code-push deployment ls astro -k"
  },
  "dependencies": {
    "flux-standard-action": "^1.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "query-string": "^4.3.4",
    "react": "16.0.0-alpha.6",
    "react-native": "~0.44.3",
    "react-native-code-push": "^2.0.2-beta",
    "react-native-fs": "^2.3.3",
    "react-native-photo-view": "^1.3.0",
    "react-native-root-toast": "^1.1.2",
    "react-native-smart-splash-screen": "^2.3.4",
    "react-native-swiper": "^1.5.4",
    "react-native-tab-view": "0.0.66",
    "react-native-vector-icons": "^4.2.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-persist": "^4.8.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.66",
    "@types/query-string": "^4.3.1",
    "@types/react": "^15.0.23",
    "@types/react-native": "^0.46.0",
    "@types/react-native-fs": "^2.0.3",
    "@types/react-native-swiper": "^1.5.7",
    "@types/react-navigation": "^1.0.12",
    "@types/react-redux": "^4.4.42",
    "@types/redux-actions": "^1.2.6",
    "babel-plugin-import": "^1.2.1",
    "babel-preset-react-native": "1.9.2",
    "tslint": "^4.3.1",
    "typescript": "^2.4.2"
  }
}
